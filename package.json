    "ts-node": "^10.4.0",
    "typescript": "~4.3.2"
  },
  "dependencies": {
    "arweave": "=1.11.4",
    "axios": "^0.27.2",
    "axios-retry": "^3.3.1",
    "commander": "^9.3.0",
    "ora": "=5.4.1",
    "tslib": "^2.4.0",
    "warp-contracts": "=1.1.14"
  },
  "resolutions": {
    "*/**/arweave": "=1.11.4"
