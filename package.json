{
  "name": "@bundlr-network/testnet-cli",
  "version": "0.0.12",
  "description": "CLI for interacting with testnet contracts",
  "type": "commonjs",
  "files": [
    "build/**/*"
  ],
  "author": "Josh Benaron <joshbenaron@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
